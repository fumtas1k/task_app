<% provide(:title, t(".title")) %>
<h1><%= t ".title" %></h1>
<%= link_to t("admin.labels.new.title"), new_admin_label_path, class: "btn btn-primary" %>
<div class="labels-container">
  <% @labels.each do |label| %>
    <%= link_to label.name, edit_admin_label_path(label), class: "label" %>
  <% end %>
</div>